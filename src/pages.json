  "globalStyle": {
    "backgroundColor": "@bgColor",
    "backgroundColorBottom": "@bgColorBottom",
    "backgroundColorTop": "@bgColorTop",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "white",
    "navigationBarTextStyle": "@navTxtStyle",
    "enablePullDownRefresh": true,
    "transparentTitle": "auto",
    "app-plus": {
      "titleNView": {
        "backgroundColor": "#1296db"
      },
      "pullToRefresh": {
        "range": "50px"
      }
    }
  },
  "tabBar": {
    "color": "@tabFontColor",
    "iconfontSrc": "static/iconfont.ttf",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "white",
    "fontSize": "12px",
    "list": [
      {
        "pagePath": "pages/index",
        "iconfont": {
          "text": "\ue602",
          "selectedText": "\ue602",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "首页"
      },
      {
        "pagePath": "pages/tripManage/tripManage",
        "iconfont": {
          "text": "\ue640",
          "selectedText": "\ue640",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "行程管理"
      },
      {
        "pagePath": "pages/insert/insert",
        "iconfont": {
          "text": "\ue60f",
          "selectedText": "\ue60f",
          "fontSize": "25px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": ""
      },
      {
        "pagePath": "pages/data/data",
        "iconfont": {
          "text": "\ue604",
          "selectedText": "\ue604",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "数据中心"
      },
      {
        "pagePath": "pages/my/my",
        "iconfont": {
          "text": "\ue626",
          "selectedText": "\ue626",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "个人中心"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  },
  "subPackages": []
}
