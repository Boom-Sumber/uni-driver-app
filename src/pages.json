        "app-plus": {
          "titleNView": {
            "titleText": "首页",
            "splitLine": {
              "color": "#9b9b9b"
            },
            "buttons": [
              {
                "type": "none",
                "fontSrc": "static/iconfont.ttf",
                "text": "\ue626",
                "fontSize": "18px",
                "color": "@navTxtStyle",
                "float": "right"
              },
              {
                "type": "none",
                "fontSrc": "static/iconfont.ttf",
                "text": "\ue6ec",
                "fontSize": "18px",
                "color": "@navTxtStyle",
                "float": "right"
              }
            ]
          }
        }
      },
      "name": "index"
    },
    {
      "path": "pages/insert/insert",
      "style": {
        "navigationBarTitleText": "新增"
      },
      "name": "insert"
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "个人中心"
      },
      "name": "my"
    },
    {
      "path": "pages/tripManage/tripManage",
      "style": {
        "navigationBarTitleText": "行程管理"
      },
      "name": "tripManage"
