{
  "pages": [
    {
      "path": "pages/index",
      "style": {
        "app-plus": {
          "titleNView": {
            "titleText": "首页",
            "buttons": [
              {
                "type": "none",
                "fontSrc": "static/iconfont/iconfont.ttf",
                "text": "",
                "fontSize": "18px",
                "fontWeight": "bold",
                "color": "@navTxtStyle",
                "float": "right"
              },
              {
                "type": "none",
                "fontSrc": "static/iconfont/iconfont.ttf",
                "text": "",
                "fontSize": "18px",
                "fontWeight": "bold",
                "color": "@navTxtStyle",
                "float": "right"
              }
            ]
          }
        }
      },
      "name": "index"
    },
    {
      "path": "pages/auth/login",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "navigationBarTextStyle": "white"
      },
      "name": "login"
    },
    {
      "path": "pages/upsert/upsert",
      "style": {
        "enablePullDownRefresh": false,
        "app-plus": {
          "titleNView": {
            "buttons": [
              {
                "type": "none",
                "fontSrc": "static/iconfont/iconfont.ttf",
                "text": "",
                "fontSize": "18px",
                "fontWeight": "bold",
                "color": "@navTxtStyle",
                "float": "right"
              }
            ]
          }
        }
      },
      "name": "upsert"
    },
    {
      "path": "pages/my/my",
      "style": {
        "enablePullDownRefresh": false,
        "navigationBarTitleText": "个人中心"
      },
      "name": "my"
    },
    {
      "path": "pages/tripManager/tripManager",
      "style": {
        "enablePullDownRefresh": false,
        "navigationBarTitleText": "行程管理"
      },
      "name": "tripManager"
    },
    {
      "path": "pages/data/data",
      "style": {
        "enablePullDownRefresh": false,
        "app-plus": {
          "titleNView": {
            "titleText": "数据中心",
            "buttons": [
              {
                "type": "none",
                "fontSrc": "static/iconfont/iconfont.ttf",
                "text": "",
                "fontSize": "18px",
                "fontWeight": "bold",
                "color": "@navTxtStyle",
                "float": "right"
              }
            ]
          }
        }
      },
      "name": "data"
    },
    {
      "path": "pages/register/register",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "navigationBarTextStyle": "white"
      },
      "name": "register"
    },
    {
      "path": "pages/tripDetail/tripDetail",
      "style": {
        "enablePullDownRefresh": false,
        "navigationBarTitleText": "行程详情"
      },
      "name": "tripDetail"
    }
  ],
  "globalStyle": {
    "enablePullDownRefresh": true,
    "navigationBarTextStyle": "@navTxtStyle",
    "transparentTitle": "auto",
    "app-plus": {
      "titleNView": {
        "backgroundColor": "@navBgColor"
      },
      "pullToRefresh": {
        "range": "20%"
      }
    }
  },
  "tabBar": {
    "color": "@tabFontColor",
    "iconfontSrc": "static/iconfont/iconfont.ttf",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "@tabBgColor",
    "fontSize": "12px",
    "list": [
      {
        "pagePath": "pages/index",
        "iconfont": {
          "text": "",
          "selectedText": "",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "首页"
      },
      {
        "pagePath": "pages/tripManager/tripManager",
        "iconfont": {
          "text": "",
          "selectedText": "",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "行程管理"
      },
      {
        "pagePath": "pages/data/data",
        "iconfont": {
          "text": "",
          "selectedText": "",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "数据中心"
      },
      {
        "pagePath": "pages/my/my",
        "iconfont": {
          "text": "",
          "selectedText": "",
          "fontSize": "16px",
          "color": "@tabFontColor",
          "selectedColor": "@tabSelectedColor"
        },
        "text": "个人中心"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  }
}
